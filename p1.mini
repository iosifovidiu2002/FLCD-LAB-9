/* Compute the maximum of three numbers */

func main(): zilch{

    var a: int
    var b: int
    var c: int

    read(a)
    read(b)
    read(c)

    assume(a > b and a > c){
        write(a)
    } 
    otherwise assume(b > c and b > a){
        write(b)
    } 
    otherwise {
        write(c)
    }
}