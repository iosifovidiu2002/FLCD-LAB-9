/* Check if a number is a palindrome */

func main(): zilch{
    
    var x: int
    var c: bool = true
    var sum: int = 0
    var temp: int = x

    read(x)

    repeat(x > 0 and x < 100){
        sum = sum + (sum * 10) + x % 10
        x = x / 10
    }

    assume(temp == sum){
        write('P')
    } otherwise {
        write('N')
    }

}